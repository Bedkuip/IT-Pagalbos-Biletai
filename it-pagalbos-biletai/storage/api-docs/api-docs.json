                "parameters": [
                    {
                        "name": "status",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "open",
                                "in_progress",
                                "resolved"
                            ]
                        }
                    },
                    {
                        "name": "assigned",
                        "in": "query",
                        "required": false,
                        "schema": {
